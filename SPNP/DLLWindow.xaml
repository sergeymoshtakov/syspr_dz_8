<Window x:Class="SPNP.DLLWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SPNP"
        mc:Ignorable="d"
        Title="DLLWindow" Height="450" Width="300">
    <StackPanel>
        <Button Content="Alert" 
                x:Name="AlertButton"
                Margin="10 7"
                Click="AlertButton_Click"/>
        <Button Content="Thread" 
                x:Name="ThreadButton"
                Margin="10 7"
                Click="ThreadButton_Click"/>
        <StackPanel Orientation="Horizontal">
            <Button Content="Sound 1" 
                x:Name="SoundButton"
                Margin="10 7"
                Click="SoundButton_Click"/>
            <Button Content="Sound 2" 
                x:Name="SoundButton1"
                Margin="10 7"
            Click="SoundButton1_Click"/>
            <Button Content="Sound 3" 
                x:Name="SoundButton2"
                Margin="10 7"
                Click="SoundButton2_Click"/>
            <Button Content="Sound 4" 
                x:Name="SoundButton4"
                Margin="10 7"
                Click="SoundButton4_Click"/>
        </StackPanel>
    </StackPanel>
</Window>
